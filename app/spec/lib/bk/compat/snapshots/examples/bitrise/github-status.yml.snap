steps:
- commands:
  - "# No need for cloning, the agent takes care of that"
  - "./build-app.sh"
  label: build
  key: build
  notify:
  - github_commit_status:
      context: custom-status
- commands:
  - "# No need for cloning, the agent takes care of that"
  - "# Warning: The 'auth_token' option is not supported in Buildkite and will be
    ignored."
  - "# Warning: The 'api_base_url' option is not supported in Buildkite and will be
    ignored."
  label: test
  key: test
  notify:
  - github_commit_status:
      context: continuous-integration/buildkite
