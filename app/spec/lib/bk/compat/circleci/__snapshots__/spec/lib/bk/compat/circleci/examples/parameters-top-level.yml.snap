---
steps:
- commands:
  - echo "Image tag used was ${IMAGETAG}"
  - echo "$(pwd) == << pipeline.parameters.workingdir >>"
  - cd << pipeline.parameters.workingdir >>
  plugins:
  - docker#v5.10.0:
      image: ubuntu:latest
  agents:
    executor_type: docker
  env:
    IMAGETAG: "<< pipeline.parameters.image-tag >>"
  key: build
