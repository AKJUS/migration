---
steps:
- commands:
  - "./setup_server.sh"
  agents:
    runs-on: ubuntu-latest
  label: ":github: setup"
  key: setup
  branches: ''
- commands:
  - "./build_server.sh"
  depends_on:
  - setup
  agents:
    runs-on: ubuntu-latest
  label: ":github: build"
  key: build
  branches: ''
- commands:
  - echo '~~~ Test server'
  - "./test_server.sh"
  - echo '~~~ Validate'
  - "./validate.sh"
  depends_on:
  - build
  agents:
    runs-on: ubuntu-latest
  label: ":github: test"
  key: test
  branches: ''
